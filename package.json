{
  "name": "gotowebinar-private-message-sender",
  "version": "1.0.0",
  "description": "Send private messages with personalized checkout links to GoToWebinar attendees",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
